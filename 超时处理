  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = '通道选择器';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = '非阻塞通道操作';
          }
          
      }
  </script>
